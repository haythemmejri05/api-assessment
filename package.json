{
  "name": "api-assessment",
  "version": "1.0.0",
  "description": "API built with Node.js and using MongoDB",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "start": "node server/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=testing jest",
    "lint": "eslint --ext .js server --color",
    "lint:fix": "eslint --fix --ext.js server",
    "prettier": "prettier server --check",
    "prettier:fix": "prettier server --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haythemmejri05/api-assessment.git"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "javascript"
  ],
  "author": "Haythem Mejri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haythemmejri05/api-assessment/issues"
  },
  "homepage": "https://github.com/haythemmejri05/api-assessment#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "@mongoosejs/double": "^0.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "log4js": "^6.6.1",
    "mongoose": "^6.6.1",
    "mongoose-int32": "^0.6.0",
    "morgan": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
